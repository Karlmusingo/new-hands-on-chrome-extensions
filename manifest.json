{
    "name": "Web Time",
    "version": "1.0",
    "description": "Show the time spent on the web: show how much time you spend on the browser by website",
    "manifest_version": 2,
    "permissions": ["activeTab", "tabCapture", "tabs", "storage"],
    "background": {
        "scripts": ["js/script.js"],
        "persistent": true
    },
    "chrome_url_overrides" : { 
        "newtab": "popup.html"
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Web Time"
    }
  }